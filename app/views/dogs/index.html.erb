<%= paginate @dogs %>

<% @dogs.each_with_index do |dog, index| %>
  <% if render_ad?(index, @ad_frequency) %>
    <%= render partial: 'advertisements/thumbnail', locals: { ad: @ad } %>
  <% end %>

  <%= render partial: 'thumbnail', locals: { dog: dog }  %>
<% end %>

<br>
<%= paginate @dogs %>
